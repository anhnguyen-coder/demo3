<table border="5" style="text-align: center">
    <tr>
        <th colspan="5">STUDENT LIST</th>
        <th><a href="/student/drop">DELETE All</a></th>
    </tr>
        <tr>
        <th>Name</th>
        <th>BirthDate</th>
        <th>Gender</th>
        <th>GPA</th>
        <th>Avatar</th>
        <th>Function</th>
    </tr>
    {{#each students }}
    <tr>
        <th>{{name}}</th>
        <th>{{ dateFormat DOB "DD/MM/YYYY"}}</th>
        <th>{{gender}}</th>
        <th>{{GPA}}</th>
        <th><img src="{{image}}" width="100" height="100"></th>
        <th><a href="/student/delete/{{_id}}">DELETE</a></th>
    </tr>
    {{/each}}
    <br>
    

</table>